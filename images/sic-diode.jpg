<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 300 300">
  <rect width="300" height="300" fill="#f0f0f0"/>
  
  <!-- Diode package representation -->
  <rect x="75" y="75" width="150" height="150" rx="20" ry="20" fill="#2c3e50" stroke="#1a252f" stroke-width="3"/>
  
  <!-- Package leads -->
  <rect x="60" y="140" width="15" height="20" fill="#7f8c8d"/>
  <rect x="225" y="140" width="15" height="20" fill="#7f8c8d"/>
  
  <!-- Internal diode symbol -->
  <path d="M120 150 L150 130 L180 150" stroke="#ecf0f1" stroke-width="8" fill="none"/>
  <line x1="120" y1="170" x2="180" y2="170" stroke="#ecf0f1" stroke-width="8"/>
  <line x1="150" y1="130" x2="150" y2="170" stroke="#ecf0f1" stroke-width="8"/>
  
  <!-- SiC marking -->
  <text x="150" y="190" font-family="Arial, sans-serif" font-size="12" fill="#ecf0f1" text-anchor="middle">
    SiC Schottky Diode
  </text>
  <text x="150" y="210" font-family="Arial, sans-serif" font-size="12" fill="#ecf0f1" text-anchor="middle">
    650V, 10A
  </text>
  
  <!-- Logo area -->
  <rect x="100" y="80" width="100" height="20" fill="#3498db" stroke="#2980b9" stroke-width="1"/>
  <text x="150" y="95" font-family="Arial, sans-serif" font-size="10" fill="#ffffff" text-anchor="middle" font-weight="bold">
    Wolfspeed
  </text>
  
  <!-- Text label -->
  <text x="150" y="250" font-family="Arial, sans-serif" font-size="14" fill="#333" text-anchor="middle">
    Wolfspeed SiC Diode
  </text>
</svg>