<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 300 300">
  <rect width="300" height="300" fill="#f0f0f0"/>
  
  <!-- LED representation -->
  <rect x="50" y="50" width="200" height="200" rx="20" ry="20" fill="#d0d0d0" stroke="#a0a0a0" stroke-width="2"/>
  
  <!-- Chip representation -->
  <rect x="100" y="100" width="100" height="100" fill="#ffcc00" stroke="#ff9900" stroke-width="1"/>
  
  <!-- Thermal pad -->
  <rect x="120" y="120" width="60" height="60" fill="#e74c3c" stroke="#c0392b" stroke-width="1"/>
  
  <!-- Electrodes -->
  <rect x="70" y="140" width="30" height="20" fill="#c0c0c0"/>
  <rect x="200" y="140" width="30" height="20" fill="#c0c0c0"/>
  
  <!-- Light rays -->
  <path d="M150 40 Q170 30, 190 40" stroke="#ffcc00" stroke-width="2" fill="none"/>
  <path d="M150 40 Q150 20, 170 25" stroke="#ffcc00" stroke-width="2" fill="none"/>
  <path d="M150 40 Q130 30, 110 40" stroke="#ffcc00" stroke-width="2" fill="none"/>
  <path d="M150 40 Q150 20, 130 25" stroke="#ffcc00" stroke-width="2" fill="none"/>
  
  <!-- Text label -->
  <text x="150" y="270" font-family="Arial, sans-serif" font-size="14" fill="#333" text-anchor="middle">
    Cree XHP70 LED
  </text>
  
  <!-- Cree logo area -->
  <rect x="80" y="60" width="140" height="20" fill="#1e40af" stroke="#1d3557" stroke-width="1"/>
  <text x="150" y="75" font-family="Arial, sans-serif" font-size="10" fill="#ffffff" text-anchor="middle" font-weight="bold">
    Cree XHP Series
  </text>
</svg>