<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 300 300">
  <rect width="300" height="300" fill="#2c3e50"/>
  
  <!-- Road representation -->
  <rect x="0" y="200" width="300" height="100" fill="#34495e"/>
  <line x1="0" y1="250" x2="300" y2="250" stroke="#f1c40f" stroke-width="2" stroke-dasharray="10,10"/>
  
  <!-- Street lamp pole -->
  <rect x="145" y="150" width="10" height="50" fill="#7f8c8d"/>
  <circle cx="150" cy="150" r="5" fill="#95a5a6"/>
  
  <!-- Street light -->
  <rect x="135" y="120" width="30" height="30" rx="5" ry="5" fill="#3498db"/>
  <circle cx="150" cy="135" r="12" fill="#f1c40f" opacity="0.7"/>
  
  <!-- Light beam -->
  <path d="M150 150 L100 250 L200 250 Z" fill="#f9e79f" opacity="0.2"/>
  
  <!-- Buildings in background -->
  <rect x="20" y="160" width="40" height="40" fill="#1a252f"/>
  <rect x="65" y="140" width="35" height="60" fill="#1a252f"/>
  <rect x="105" y="170" width="30" height="30" fill="#1a252f"/>
  <rect x="240" y="150" width="40" height="50" fill="#1a252f"/>
  <rect x="200" y="165" width="35" height="35" fill="#1a252f"/>
  
  <!-- Moon -->
  <circle cx="250" cy="50" r="20" fill="#f7dc6f"/>
  <circle cx="255" cy="45" r="3" fill="#d6d3ce"/>
  <circle cx="245" cy="50" r="2" fill="#d6d3ce"/>
  <circle cx="252" cy="55" r="2" fill="#d6d3ce"/>
  
  <!-- Stars -->
  <circle cx="50" cy="30" r="2" fill="#fdfd96"/>
  <circle cx="80" cy="60" r="2" fill="#fdfd96"/>
  <circle cx="120" cy="20" r="2" fill="#fdfd96"/>
  <circle cx="200" cy="40" r="2" fill="#fdfd96"/>
  <circle cx="220" cy="80" r="2" fill="#fdfd96"/>
  
  <!-- Text label -->
  <text x="150" y="290" font-family="Arial, sans-serif" font-size="14" fill="#ecf0f1" text-anchor="middle">
    Outdoor Lighting Solution
  </text>
  
  <!-- Cree LED symbol -->
  <circle cx="20" cy="20" r="8" fill="#3498db"/>
  <path d="M16 16 Q20 12, 24 16 Q20 20, 16 16" fill="#f1c40f" opacity="0.7"/>
</svg>